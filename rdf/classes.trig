prefix foaf: <http://xmlns.com/foaf/0.1/>
prefix geo: <http://www.opengis.net/ont/geosparql#>
prefix owl: <http://www.w3.org/2002/07/owl#>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix schema: <https://schema.org/>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>

prefix id: <https://iisg.amsterdam/id/diamantbewerkers/>
prefix vocab: <https://iisg.amsterdam/vocab/diamantbewerkers/>

vocab: {
  vocab:Examination
    rdfs:label
      "examination"@en,
      "examen"@nl;
    rdfs:subClassOf schema:Event.

  vocab:Membership
    rdfs:label
      "membership"@en,
      "lidmaatschap"@nl;
    rdfs:subClassOf schema:Event.

  vocab:Occupation
    owl:oneOf
      ( id:brillantslijpen
        id:brillantsnijden
        id:brillantverstellen
        id:kapbewerken
        id:klooven
        id:roosjesslijpen
        id:roosjessnijden
        id:roosjesverstellen
        id:zagen );
    rdfs:label
      "occupation"@en,
      "beroep"@nl;
    rdfs:subClassOf schema:Occupation.

  vocab:Residency
    rdfs:label
      "residency"@en,
      "bewoning"@nl;
    rdfs:subClassOf schema:Event.

  vocab:Section
    rdfs:label
      "section"@en,
      "afdeling"@nl;
    rdfs:subClassOf schema:Organization.

  vocab:Subsection
    rdfs:label
      "subsection"@en,
      "onderafdeling"@nl;
    rdfs:subClassOf schema:Organization.

  vocab:Union
    rdfs:label
      "labor union"@en,
      "vakbond"@nl;
    rdfs:subClassOf schema:Organization.

  vocab:WorkEvent
    rdfs:label
      "work event"@en,
      "werk evenement"@nl;
    rdfs:subClassOf schema:Event.

  vocab:Workshop
    rdfs:label
      "workshop"@en,
      "werkplaats"@nl;
    rdfs:subClassOf schema:Organization.
}
