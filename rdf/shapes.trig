  id:biographyGroup
    a sh:PropertyGroup;
    rdfs:comment "Groups of properties with biographical information about the card holder."@en.

  id:cardGroup
    a sh:PropertyGroup;
    rdfs:comment "Groups of properties that together form the contents of a person's card."@en.

  id:cardNameGroup
    a sh:PropertyGroup;
    rdfs:comment "Group of properties that specify a person's name, as represented on their card."@en.

  id:childGroup
    a sh:PropertyGroup;
    rdfs:comment "Group of properties that describe the card holder's child(ren)."@en.

  id:dismission
    a sh:PropertyGroup;
    rdfs:comment "Group of properties that describe the dismission of the card holder from the union."@en.

  id:educationGroup
    a sh:PropertyGroup;
    rdfs:comment "Groups of properties that describe the card holder's formal education in the diamond industry."@en.

  id:factoryGroup
    a sh:PropertyGroup;
    rdfs:comment "Group of properties that the describe a diamond factory as part of an internship."@en.

  id:internshipGroup
    a sh:PropertyGroup;
    rdfs:comment "Group of properties that describe the place and time of the internship of the card holder."@en.

  id:masterGroup
    a sh:PropertyGroup;
    rdfs:comment "Group of properties that describe the name(s) and union membership of the internship's master."@en.

  id:membershipGroup
    a sh:PropertyGroup;
    rdfs:comment "Information about the Union membership of the card holder."@en.

  id:migrationGroup
    a sh:PropertyGroup;
    rdfs:comment "Group of properties that describe the card holder's migration to Antwerp and back."@en.

  id:parentGroup
    a sh:PropertyGroup;
    sh:description "Group of properties that describe the parent of the card holder"@en.

  id:personGroup
    a sh:PropertyGroup;
    rdfs:comment "Groups of properties with biographical information about a person."@en.

  id:personNameGroup
    a sh:PropertyGroup;
    rdfs:comment "Group of properties that are used to specify a person's name."@en.

  id:readmissionGroup
    a sh:PropertyGroup;
    rdfs:comment "Group of properties that describe the readmission of the card holder to the union."@en.

  id:residenceGroup
    a sh:PropertyGroup;
    rdfs:comment "Groups of properties that describe the housing addresses and move dates of the card holder."@en.

  id:streetGroup
    a sh:PropertyGroup;
    rdfs:comment "The name of a street as a string and, where available, the Adamlink identifier of the street"@en.
