prefix foaf: <http://xmlns.com/foaf/0.1/>
prefix org: <http://www.w3.org/ns/org#>
prefix owl: <http://www.w3.org/2002/07/owl#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix schema: <https://schema.org/>

prefix andb: <https://iisg.amsterdam/vocab/adb-andb/>

andb: {
  andb:AdbCard
    a owl:Class;#
    rdfs:comment
      "A card that describes a membership at the Antwerp Diamondworker Union"@en,
      "Een kaart die een lidmaatschap bij de Antwerpse Diamantbewerkersbond beschrijft."@nl;
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "ADB membership card"@en,
      "ADB lidmaatschapskaart"@nl;
    rdfs:subClassOf andb:MembershipCard.

  andb:AndbCard
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "A card that describes a membership at the General Diamond Workers' Union of the Netherlands."@en,
      "Een kaart die een lidmaatschap bij de Algemene Nederlandse Diamantbewerkersbond beschrijft."@nl;
    rdfs:comment
      "ANDB membership card"@en,
      "ANDB lidmaatschapskaart"@nl;
    rdfs:subClassOf andb:MembershipCard.

  andb:ApprenticeCard
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "A card that describes that apprenticeship of a specific person at a labor union."@en,
      "Een kaart die een leertraject van een specifieke leerling bij een vakbond beschrijft."@nl;
    rdfs:comment
      "Apprentice card"@en,
      "Leerlingkaart"@nl;
    rdfs:subClassOf andb:Card.

  andb:Card
    a owl:Class;#
    rdfs:comment
      "A card that registers properties of a specific person."@en,
      "Een kaart waarop aspecten van een specifieke persoon zijn vastgelegd."@nl;
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Card"@en,
      "Kaart"@nl;
    rdfs:subClassOf foaf:Document.

  andb:Diamantslijperij
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label "Diamantslijperij"@nl;
    rdfs:subClassOf andb:Workshop.

  andb:Diamantzagerij
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label "Diamantzagerij"@nl;
    rdfs:subClassOf andb:Workshop.

  andb:Dismission
    a owl:Class;#
    rdfs:comment
      "An event in which the membership at a union is terminated."@en,
      "Een evenement waarbij het lidmaatschap bij een bond wordt ingetrokken."@nl;
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Dismission"@en,
      "Ontslag"@nl;
    rdfs:subClassOf andb:Mutation.

  andb:Education
    a owl:Class;#
    rdfs:comment
      "An education program."@en,
      "Een oderwijstraject."@nl;
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Education"@en,
      "Onderwijs"@nl;
    rdfs:subClassOf schema:Event.

  andb:Internship
    a owl:Class;#
    rdfs:comment
      "An internship at a diamond factory, as part of an education program."@en,
      "Een stage bij een diamantslijperij of diamantzagerij, als onderdeel van een onderwijstraject."@nl;
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Internship"@en,
      "Stage"@nl;
    rdfs:subClassOf schema:Event.

  andb:Master
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Master"@en,
      "Meester"@nl;
    rdfs:subClassOf schema:Person.

  andb:MembershipCard
    a owl:Class;#
    rdfs:comment
      "A card describing the membership of a specific person at a labor union."@en,
      "Een kaart die het lidmaatschap van een specifieke persoon bij een vakbond beschrijft."@nl;
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Membership card"@en,
      "Lidmaatschapskaart"@nl;
    rdfs:subClassOf andb:Card.

  andb:Migration
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Migration"@en,
      "Migratie"@nl;
    rdfs:subClassOf schema:Event.

  andb:Mutation
    a owl:Class;#
    rdfs:comment
      "A change to a union membership."@en,
      "Een verandering van een lidmaatschap bij een bond."@nl;
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Membership mutation"@en,
      "Lidmaatschapsverandering"@en;
    rdfs:subClassOf schema:Event.

  andb:Readmission
    a owl:Class;#
    rdfs:comment
      "A readmission event of a previous member to a union."@en,
      "De wedertoelating van een voormalig lid bij een bond."@nl;
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Readmission"@en,
      "Wedertoelating"@nl;
    rdfs:subClassOf andb:Mutation.

  andb:Residency
    a owl:Class;#
    rdfs:comment
      "A residency event of a person at a certain address."@en,
      "De bewoning van een huis of appartement door een persoon."@nl;
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Residency"@en,
      "Bewoning"@nl;
    rdfs:subClassOf schema:Event.

  andb:Union
    a owl:Class;#
    rdfs:comment
      "A labor union."@en,
      "Een vakbond."@nl;
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Labor union"@en,
      "Vakbond"@nl;
    rdfs:subClassOf org:FormalOrganization.

  andb:Withdrawal
    a owl:Class;#
    rdfs:comment
      "The termination of a union membership by a member."@en,
      "De opzegging van een lidmaatschap bij een bond."@nl;
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Membership withdrawal"@en,
      "Opzegging lidmaatschap"@nl;
    rdfs:subClassOf andb:Mutation.

  andb:Workshop
    a owl:Class;#
    rdfs:comment
      "A workshop where diamonds are handled."@en,
      "Een werkplaats waar met diamanten gewerkt wordt."@nl;
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Workshop"@en,
      "Werkplaats"@nl;
    rdfs:subClassOf org:FormalOrganization.
}
