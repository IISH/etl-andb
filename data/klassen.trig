prefix foaf: <http://xmlns.com/foaf/0.1/>
prefix geo: <http://www.opengis.net/ont/geosparql#>
prefix org: <http://www.w3.org/ns/org#>
prefix owl: <http://www.w3.org/2002/07/owl#>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix schema: <https://schema.org/>
prefix skos: <http://www.w3.org/2004/02/skos/core#>
prefix time: <http://www.w3.org/2006/time#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>

prefix andb: <https://iisg.amsterdam/vocab/adb-andb/>
prefix id: <https://iisg.amsterdam/id/adb-andb/>

andb: {
  andb:Card
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Card"@en,
      "Kaart"@nl;
    rdfs:subClassOf foaf:Document.

  andb:Education
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Education"@en,
      "Onderwijs"@nl;
    rdfs:subClassOf schema:Event.

  andb:Examination
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Examination"@en,
      "Examen"@nl;
    rdfs:subClassOf schema:Event.

  andb:Internship
    a owl:Class;#
    rdfs:comment "Internship at diamond factory as part of the formal education."@en;
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Internship"@en,
      "Stage"@nl;
    rdfs:subClassOf schema:Event.

  andb:MembershipCard
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Membership card"@en,
      "Lidmaatschapskaart"@nl;
    rdfs:subClassOf andb:Card.

  andb:Migration
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Migration"@en,
      "Migratie"@nl;
    rdfs:subClassOf schema:Event.

  andb:Residency
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Residency"@en,
      "Bewoning"@nl;
    rdfs:subClassOf schema:Event.

  andb:StudentCard
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Student card"@en,
      "Leerlingkaart"@nl;
    rdfs:subClassOf andb:Card.

  andb:Union
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Labor union"@en,
      "Vakbond"@nl;
    rdfs:subClassOf org:FormalOrganization.

  andb:WorkEvent
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "work event"@en,
      "werk evenement"@nl;
    rdfs:subClassOf schema:Event.

  andb:Workshop
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Workshop"@en,
      "Werkplaats"@nl;
    rdfs:subClassOf schema:FormalOrganization.
}
