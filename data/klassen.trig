prefix foaf: <http://xmlns.com/foaf/0.1/>
prefix geo: <http://www.opengis.net/ont/geosparql#>
prefix org: <http://www.w3.org/ns/org#>
prefix owl: <http://www.w3.org/2002/07/owl#>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix schema: <https://schema.org/>
prefix skos: <http://www.w3.org/2004/02/skos/core#>
prefix time: <http://www.w3.org/2006/time#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>

prefix andb: <https://iisg.amsterdam/vocab/adb-andb/>
prefix id: <https://iisg.amsterdam/id/adb-andb/>

andb: {
  andb:AdbCard
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "ADB membership Card"@en,
      "ADB lidmaatschapskaart"@nl;
    rdfs:comment
      "Member of ADB"@en,
      "Lid ADB"@nl;
    rdfs:subClassOf andb:Card.

  andb:AndbCard
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "ANDB membership Card"@en,
      "ANDB lidmaatschapskaart"@nl;
    rdfs:comment
      "Member of ANDB"@en,
      "Lid ANDB"@nl;
    rdfs:subClassOf andb:Card.

  andb:ApprenticeCard
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Apprentice Card"@en,
      "Leerlingkaart"@nl;
    rdfs:comment
      "Apprentice"@en,
      "Leerling"@nl;
    rdfs:subClassOf andb:Card.

  andb:Card
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Card"@en,
      "Kaart"@nl;
    rdfs:subClassOf foaf:Document.

  andb:Dismission
    a owl:Class;#
    rdfs:comment "The membership is ended by the union."@en;
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Dismission"@en,
      "Ontslag"@nl;
    rdfs:subClassOf andb:Mutation.

  andb:Education
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Education"@en,
      "Onderwijs"@nl;
    rdfs:subClassOf schema:Event.

  andb:Internship
    a owl:Class;#
    rdfs:comment "Internship at diamond factory as part of the formal education."@en;
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Internship"@en,
      "Stage"@nl;
    rdfs:subClassOf schema:Event.

  andb:Migration
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Migration"@en,
      "Migratie"@nl;
    rdfs:subClassOf schema:Event.

  andb:Mutation
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Membership mutation"@en,
      "Lidmaatschapsverandering"@en;
    rdfs:subClassOf schema:Event.

  andb:Readmission
    a owl:Class;#
    rdfs:comment "The member is readmitted by the union union."@en;
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Readmission"@en,
      "Wedertoelating"@nl;
    rdfs:subClassOf andb:Mutation.

  andb:Residency
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Residency"@en,
      "Bewoning"@nl;
    rdfs:subClassOf schema:Event.

  andb:Union
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Labor Union"@en,
      "Vakbond"@nl;
    rdfs:subClassOf org:FormalOrganization.

  andb:Withdrawal
    a owl:Class;#
    rdfs:comment "The member ends his membership of the union."@en;
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Membership withdrawal"@en,
      "Opzegging lidmaatschap"@nl;
    rdfs:subClassOf andb:Mutation.

  andb:Workshop
    a owl:Class;#
    rdfs:isDefinedBy andb:;#
    rdfs:label
      "Workshop"@en,
      "Werkplaats"@nl;
    rdfs:subClassOf org:FormalOrganization.
}
