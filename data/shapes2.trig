prefix bio: <http://purl.org/vocab/bio/0.1/>
prefix hg: <http://rdf.histograph.io/>
prefix dct: <http://purl.org/dc/terms/>
prefix foaf: <http://xmlns.com/foaf/0.1/>
prefix geo: <http://www.opengis.net/ont/geosparql#>
prefix owl: <http://www.w3.org/2002/07/owl#>
prefix pnv: <https://w3id.org/pnv#>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix schema: <https://schema.org/>
prefix sh: <http://www.w3.org/ns/shacl#>
prefix time: <http://www.w3.org/2006/time#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>

prefix id: <https://iisg.amsterdam/id/diamantbewerkers/>
prefix vocab: <https://iisg.amsterdam/vocab/diamantbewerkers/>

id:shapesGraph {
  vocab:AddressShape
    sh:property vocab:labelShape;
    sh:targetClass schema:PostalAddress.

  vocab:labelShape
    sh:datatype rdf:langString;
    sh:description "We try to give every node a human-readable label."@en;
    sh:languageIn ( "en" "nl" );
    sh:minCount 1;
    sh:name "has label"@en;
    sh:path rdfs:label;
    sh:uniqueLang true.
}
