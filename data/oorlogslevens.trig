prefix dct: <http://purl.org/dc/terms/>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix sh: <http://www.w3.org/ns/shacl#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>

prefix id: <https://iisg.amsterdam/id/adb-andb/>
prefix andb: <https://iisg.amsterdam/vocab/adb-andb/>

id:shapesGraph {
  id:deathDate_oorlogslevens
    dct:source "unique/oorlogslevens_deathDate";
    sh:datatype xsd:date;
    sh:description """
<section lang="en">
  <p>The person's death date according to <a href="https://www.oorlogslevens.nl" target="_blank">Oorlogslevens</a>.</p>
</section>"""^^rdf:HTML;
    sh:name "has death date according to oorlogslevens.nl"@en;
    sh:path andb:claimedDeathDate.

  id:deathPlace_oorlogslevens
    dct:source "unique/oorlogslevens_deathPlace";
    sh:datatype xsd:string;
    sh:description """
<section lang="en">
  <p>The person's place of death according to <a href="https://www.oorlogslevens.nl" target="_blank">Oorlogslevens</a>.</p>
</section>"""^^rdf:HTML;
    sh:name "has death place"@en;
    sh:path andb:claimedDeathPlace.
}
