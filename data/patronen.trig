      "apprentice/sdn_middle_name",
      "unique/middle_name";
    rdfs:isDefinedBy andb:;#
    sh:datatype xsd:string;
    sh:description
      "The person's middle name."@en,
      "todo"@nl;
    sh:name
      "has middle name"@en,
      "todo"@nl;
    sh:maxCount 1;
    sh:minLength 1;#
    sh:nodeKind sh:Literal;#
    sh:path schema:additionalName.

  andb:birthDateShape
    a sh:PropertyShape;#
    dct:source
