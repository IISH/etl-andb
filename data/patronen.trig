prefix bio: <http://purl.org/vocab/bio/0.1/>
prefix cshapes: <https://iisg.amsterdam/vocab/>
prefix hg: <http://rdf.histograph.io/>
prefix dct: <http://purl.org/dc/terms/>
prefix foaf: <http://xmlns.com/foaf/0.1/>
prefix geo: <http://www.opengis.net/ont/geosparql#>
prefix org: <http://www.w3.org/ns/org#>
prefix owl: <http://www.w3.org/2002/07/owl#>
prefix pnv: <https://w3id.org/pnv#>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix schema: <https://schema.org/>
prefix sh: <http://www.w3.org/ns/shacl#>
prefix skos: <http://www.w3.org/2004/02/skos/core#>
prefix time: <http://www.w3.org/2006/time#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>

prefix andb: <https://iisg.amsterdam/vocab/adb-andb/>
prefix id: <https://iisg.amsterdam/id/andb/>

andb: {
  andb:durationShape
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:class time:ProperInterval;#
    sh:maxCount 1;
    sh:nodeKind sh:IRI;#
    sh:path andb:duration.

  andb:labelShape
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:datatype xsd:string;
    sh:description
      "Nodes that have human-readable labels, but not in a specific language."@en,
      "Nodes die een menselijk leesbaar label hebben, maar niet in een specifieke taal."@nl;
    sh:maxCount 1;
    sh:minCount 1;
    sh:minLength 1;#
    sh:name
      "has human-readable label"@en,
      "heeft menselijk leesbaar label"@nl;
    sh:nodeKind sh:Literal;#
    sh:path rdfs:label.

  andb:labelShape_en_nl
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:datatype rdf:langString;#
    sh:description
      "Nodes that have human-readable labels in Engels and Dutch."@en,
      "Nodes die een menselijk leesbaar label in het Engels en het Nederlands bevatten."@nl;
    sh:languageIn ( "en" "nl" );
    sh:maxCount 2;
    sh:minCount 2;
    sh:minLength 1;#
    sh:name
      "has human-readable label"@en,
      "heeft menselijk leesbaar label"@nl;
    sh:nodeKind sh:Literal;#
    sh:path rdfs:label;
    sh:uniqueLang true.#

  andb:labelShape_nl
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:datatype rdf:langString;#
    sh:description
      "Nodes that have a human-readable label in Dutch."@en,
      "Nodes die een menselijk leesbaar label in het Nederlands bevatten."@nl;
    sh:languageIn ( "nl" );
    sh:maxCount 1;
    sh:minCount 1;
    sh:minLength 1;#
    sh:name
      "has human-readable label"@en,
      "heeft menselijk leesbaar label"@nl;
    sh:nodeKind sh:Literal;#
    sh:path rdfs:label;
    sh:uniqueLang true.#

  andb:nameShape
    a sh:PropertyShape;#
    dct:source
      "adb/mbr_city_01 t/m 5",
      "andb/mbr_city-01 t/m 26",
      "apprentice/sdn_city-01 t/m 5",
      "apprentice/wsp_city-01 t/m 10",
      "wsp_name-01 t/m 10";
    rdfs:isDefinedBy andb:;#
    sh:datatype rdf:langString;#
    sh:languageIn ( "nl" );
    sh:maxCount 1;
    sh:minCount 1;
    sh:minLength 1;#
    sh:name
      "has name"@en,
      "heeft naam"@nl;
    sh:path schema:name;
    sh:uniqueLang true.#


  andb:AddressShape
    a sh:NodeShape;#
    sh:closed true;#
    sh:ignoredProperties ( rdf:type );#
    sh:property
      andb:Address_identifier,
      andb:houseNumberAdditionShape,
      andb:houseNumberShape,
      andb:labelShape_nl,
      andb:streetShape;
    sh:targetClass schema:PostalAddress.

  andb:Address_identifier
    a sh:PropertyShape;#
    dct:source
      "andb/mbr_address_locationPoint-01 t/m 26",
      "apprentice/sdn_address_locationPoint-01 t/m 5";
    rdfs:isDefinedBy andb:;#
    sh:datatype xsd:string;
    sh:description "The HISGIS identifier of the (Amsterdam) address."@en;
    sh:minLength 1;#
    sh:name "has hisgis identifier"@en;
    sh:nodeKind sh:Literal;#
    sh:path dct:identifier.

  andb:houseNumberAdditionShape
    a sh:PropertyShape;
    dct:source
      "adb/mbr_street_no_add-01 t/m 5",
      "andb/mbr_street_no_add-01 t/m 26",
      "apprentice/sdn_street_no_add-01 t/m 5",
      "apprentice/wsp_street_no_ad-01 t/m 10";
    rdfs:isDefinedBy andb:;#
    sh:datatype xsd:string;
    sh:maxCount 1;
    sh:minLength 1;#
    sh:nodeKind sh:Literal;#
    sh:path andb:houseNumberAddition.

  andb:houseNumberShape
    a sh:PropertyShape;#
    dct:source
      "adb/mbr_street_no_01 t/m 5",
      "andb/mbr_street_no-01 t/m 26",
      "apprentice/sdn_street_no-01 t/m 5",
      "apprentice/wsp_street_no-01 t/m 10";
    rdfs:isDefinedBy andb:;#
    sh:datatype xsd:string;
    sh:maxCount 1;
    sh:minLength 1;#
    sh:nodeKind sh:Literal;#
    sh:path andb:houseNumber.

  andb:streetShape
    a sh:PropertyShape;#
    dct:source
      "adb/mbr_street_01 t/m 5",
      "andb/mbr_street-01 t/m 26",
      "apprentice/sdn_street-01 t/m 5",
      "apprentice/wsp_street-01 t/m 10";
    rdfs:isDefinedBy andb:;#
    sh:class hg:Street;#
    sh:description "The street part of the address."@en;#
    sh:name "has street"@en;#
    sh:maxCount 1;
    sh:minCount 1;
    sh:nodeKind sh:IRI;#
    sh:path andb:street.


  andb:CardShape
    a sh:NodeShape;#
    rdfs:isDefinedBy andb:;#
    sh:closed true;#
    sh:ignoredProperties ( rdf:type );#
    sh:property
      andb:CardShape_identifier,
      andb:CardShape_primaryTopic,
      andb:imageShape,
      andb:labelShape_en_nl;
    sh:targetClass andb:MembershipCard.#adb-andb:Card

  andb:CardShape_identifier
    a sh:PropertyShape;#
    dct:source
      "adb/card_id1",
      "andb/card_id1 t/m 4",
      "apprentice/card_id1 t/m 2";
    rdfs:isDefinedBy andb:;#
    sh:datatype xsd:string;
    sh:description "Het administratieve nummer van de kaart."@nl;
    sh:minLength 1;#
    sh:name
      "has card identifier"@en,
      "heeft kaartnummer"@nl;
    sh:path dct:identifier;
    sh:nodeKind sh:Literal;#
    sh:pattern "^[0-9]+$".

  andb:CardShape_primaryTopic
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:class schema:Person;
    sh:description
      "Cards describe persons."@en,
      "Kaarten beschrijven personen."@nl;
    sh:maxCount 1;
    sh:minCount 1;
    sh:name
      "is about"@en,
      "gaat over"@nl;
    sh:nodeKind sh:IRI;#
    sh:path foaf:primaryTopic.

  andb:imageShape
    a sh:PropertyShape;#
    dct:source
      "adb/adm_image_uuid",
      "andb/adm_image_uuid",
      "apprentice/adm_image_uuid";
    rdfs:isDefinedBy andb:;#
    sh:class schema:ImageObject;
    sh:description
      "Cards have one scanned image."@en,
      "Kaarten hebben precies één ingescande afbeelding."@nl;
    sh:maxCount 1;
    sh:minCount 1;
    sh:name
      "has image"@en,
      "heeft afbeelding"@nl;
    sh:nodeKind sh:IRI;#
    sh:path schema:image.


  andb:EducationShape
    a sh:NodeShape;#
    rdfs:isDefinedBy andb:;#
    sh:closed true;#
    sh:ignoredProperties ( rdf:type );#
    sh:property
      andb:EducationShape_contains,
      andb:durationShape,
      andb:examinatorShape,
      andb:internshipShape,
      andb:labelShape_en_nl,
      andb:masterShape,
      andb:occupationShape,
      andb:remarkShape,
      andb:studentShape;
    sh:targetClass andb:Education.

  andb:EducationShape_contains
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:class andb:Internship;
    sh:minCount 1;
    sh:nodeKind sh:IRI;
    sh:path
      ( andb:duration
        time:intervalContains
        [ sh:inversePath andb:duration ] ).

  andb:examinatorShape
    a sh:PropertyShape;#
    dct:source "apprentice/edu_examinator";
    rdfs:isDefinedBy andb:;#
    sh:class xsd:string;#
    sh:description
      "The full name of the examinator."@en,
      "De volledige naam van de examinator."@nl;
    sh:minLength 1;#
    sh:nodeKind sh:Literal;#
    sh:path andb:examinator.

  andb:internshipShape
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:class andb:Internship;#
    sh:minCount 1;
    sh:name
      "has internship"@en,
      "volgt stage"@nl;
    sh:nodeKind sh:IRI;#
    sh:path andb:internship.

  andb:masterShape
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:datatype xsd:string;#
    sh:maxCount 1;
    sh:minLength 1;#
    sh:nodeKind sh:Literal;#
    sh:path andb:master.

  andb:occupationShape
    a sh:PropertyShape;#
    dct:source: "apprentice/sdn_occupation";
    rdfs:isDefinedBy andb:;#
    sh:class schema:Occupation;
    sh:description "Occupation the education is preparing for"@en;
    sh:in
      ( id:brillantslijpen
        id:brillantsnijden
        id:brillantverstellen
        id:kapbewerken
        id:klooven
        id:roosjesslijpen
        id:roosjessnijden
        id:roosjesverstellen
        id:zagen );
    sh:name
      "has occupation"@en,
      "heeft beroep"@nl;
    sh:nodeKind sh:IRI;#
    sh:path schema:occupation.

  andb:remarkShape
    a sh:PropertyNode;#
    rdfs:isDefinedBy andb:;#
    sh:datatype rdf:langString;#
    sh:maxCount 1;
    sh:minCount 1;
    sh:languagesIn ( "nl" );
    sh:nodeKind sh:Literal;#
    sh:path andb:remark.

  andb:studentShape
    a sh:PropertyNode;#
    rdfs:isDefinedBy andb:;#
    sh:class schema:Person;
    sh:description "The student's education"@en;
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "has student"@en;
    sh:nodeKind sh:IRI;#
    sh:path andb:student.


  andb:ImageShape
    a sh:NodeShape;#
    rdfs:isDefinedBy andb:;#
    sh:closed true;#
    sh:ignoredProperties ( rdf:type );#
    sh:property
      andb:contentSizeShape,
      andb:contentUrlShape,
      andb:md5Shape,
      andb:mediaTypeShape,
      andb:screenVersionShape,
      andb:thumbnailShape,
      andb:labelShape_en_nl;
    sh:targetClass schema:ImageObject.

  andb:contentSizeShape
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:datatype xsd:positiveInteger;
    sh:description "The size of the binary image in bytes."@en;
    sh:maxCount 1;
    sh:minCount 1;
    sh:nodeKind sh:Literal;#
    sh:path schema:contentSize.

  andb:contentUrlShape
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:datatype xsd:anyURI;#
    sh:description "Every image has exactly one URL where it can be downloaded."@en;
    sh:maxCount 1;
    sh:minCount 1;
    sh:minLength 1;#
    sh:nodeKind sh:Literal;#
    sh:path schema:contentUrl;
    sh:pattern "^https://hdl.handle.net/[0-9]{5}/[0-9A-F]{8}-([0-9A-F]{4}-){3}[0-9A-F]{12}\\?locatt=view:(level2|level3|master)$".

  andb:md5Shape
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:datatype xsd:string;#
    sh:maxCount 1;
    sh:minCount 1;
    sh:minLength 1;#
    sh:nodeKind sh:Literal;#
    sh:path andb:md5;
    sh:pattern "^[0-9a-f]{32}$".

  andb:mediaTypeShape
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:datatype xsd:string;
    sh:description "The Media Type that is used to encoding the image (JPEG or TIFF)."@en;
    sh:maxCount 1;
    sh:minCount 1;
    sh:minLength 1;#
    sh:nodeKind sh:Literal;#
    sh:path schema:encodingFormat;
    sh:pattern "^image/(jpeg|tiff)$".

  andb:screenVersionShape
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:class schema:ImageObject;#
    sh:description "Some images have a screen friendly version."@en;
    sh:maxCount 1;
    sh:nodeKind sh:IRI;#
    sh:path andb:screenVersion.

  andb:thumbnailShape
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:class schema:ImageObject;
    sh:description "Some images have a thumbnail version."@en;
    sh:maxCount 1;
    sh:nodeKind sh:IRI;#
    sh:path schema:thumbnail.


  andb:Instant
    a sh:NodeShape;#
    rdfs:isDefinedBy andb:;#
    sh:closed true;#
    sh:ignoredProperties ( rdf:type );#
    sh:name
      "Instant"@en,
      "Moment"@nl;
    sh:property
      andb:inXSDDate,
      andb:labelShape;
    sh:targetClass time:Instant.

  andb:inXSDDate
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:datatype xsd:date;
    sh:maxCount 1;
    sh:minCount 1;
    sh:name
      "has XSD date"@en,
      "heeft XSD datum"@nl;
    sh:nodeKind sh:Literal;#
    sh:path time:inXSDDate;
    sh:pattern "^[0-9]{4}-[0-9]{2}-[0-9]{2}$".


  andb:InternshipShape
    a sh:NodeShape;
    sh:closed true;#
    sh:ignoredProperties ( rdf:type );#
    sh:property
      andb:durationShape,
      andb:duringShape,
      andb:labelShape_en_nl,
      andb:workshopShape;
    sh:targetClass andb:Internship.

  andb:duringShape
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:class andb:Education;
    sh:minCount 1;
    sh:maxCount 1;
    sh:nodeKind sh:IRI;#
    sh:path
      ( andb:duration
        time:intervalDuring
        [ sh:inversePath andb:duration ] ).

  andb:workshopShape
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:class andb:Workshop;
    sh:maxCount 1;
    sh:name
      "has workshop"@en,
      "heeft werkplaats"@nl;
    sh:nodeKind sh:IRI;#
    sh:path org:organization.


  andb:IntervalShape
    a sh:NodeShape;#
    rdfs:isDefinedBy andb:;#
    sh:closed true;#
    sh:ignoredProperties ( rdf:type );#
    sh:name
      "Interval"@en,
      "Interval"@nl;
    sh:property
      andb:IntervalShape_contains,
      andb:IntervalShape_during,
      andb:afterShape,
      andb:beforeShape,
      andb:beginShape,
      andb:endShape,
      andb:labelShape;
    sh:targetClass time:ProperInterval.

  andb:IntervalShape_contains
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:class time:ProperInterval;
    sh:nodeKind sh:IRI;#
    sh:path time:intervalContains.

  andb:IntervalShape_during
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:class time:ProperInterval;
    sh:nodeKind sh:IRI;#
    sh:path time:intervalDuring.

  andb:afterShape
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:class time:ProperInterval;
    sh:description
      "Another interval that occurred directly after this one."@en,
      "Een ander interval dat direct na dit interval komt."@nl;
    sh:name
      "occurs after"@en,
      "komt na"@nl;
    sh:nodeKind sh:IRI;#
    sh:path time:after;
    skos:usageNote "Hier moet gebruik worden gemaakt van de volgnummers, niet van de datum."@nl.

  andb:beforeShape
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:class time:ProperInterval;
    sh:description
      "Another interval that occurred directly before this one."@en,
      "Een ander interval dat direct voor dit interval komt."@nl;
    sh:maxCount 1;
    sh:name
      "occurs before"@en,
      "gaat vooraf aan"@nl;
    sh:nodeKind sh:IRI;#
    sh:path time:before;
    skos:usageNote "Hier moet gebruik worden gemaakt van de volgnummers, niet van de datum."@nl.

  andb:beginShape
    a sh:PropertyShape;#
    dct:source
      "apprentice/admission_day/month/year-01",
      "adb/mmb_day/month/year-01",
      "andb/departure_day/month/year_01 t/m 08",
      "andb/mbr_addr_day/month/year 1 t/m 26",
      "andb/mmb_day/month/year-01 t/m 07",
      "apprentice/mmb_day/month/year",
      "apprentice/sdn_addr_day/month/year 1 t/m 25",
      "apprentice/wsp_day/month/year-01 t/m 10";
    rdfs:isDefinedBy andb:;#
    sh:class time:Instant;
    sh:name
      "has start date"@en,
      "heeft begindatum"@nl;
    sh:maxCount 1;
    sh:nodeKind sh:IRI;#
    sh:path time:hasBeginning.

  andb:endShape
    a sh:PropertyShape;#
    dct:source
      "andb/return_day/month/year_01 t/m 08",
      "apprentice/edu_end_day/month/year-01";
    rdfs:isDefinedBy andb:;#
    sh:class time:Instant;
    sh:maxCount 1;
    sh:name
      "has end date"@en,
      "heeft einddatum"@nl;
    sh:nodeKind sh:IRI;#
    sh:path time:hasEnd.


  andb:MembershipShape
    a sh:NodeShape;#
    rdfs:isDefinedBy andb:;#
    sh:closed true;#
    sh:ignoredProperties ( rdf:type );#
    sh:property
      andb:Membership_identifier,
      andb:labelShape_en_nl,
      andb:memberShape,
      andb:memberDuringShape,
      andb:organizationShape;
    sh:targetClass org:Membership.

  andb:Membership_identifier
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:datatype xsd:string;
    sh:maxCount 1;
    sh:minCount 1;
    sh:minLength 1;#
    sh:nodeKind sh:Literal;#
    sh:path dct:identifier.

  andb:memberShape
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:class schema:Person;
    sh:description
      "todo"@en,
      "De persoon waarop het lidmaatschap van toepassing is."@nl;
    sh:minCount 1;
    sh:maxCount 1;
    sh:name
      "has member"@en,
      "heeft lid"@nl;
    sh:nodeKind sh:IRI;#
    sh:path org:member.

  andb:memberDuringShape
    a sh:PropertyShape;
    rdfs:isDefinedBy andb:;#
    sh:class time:ProperInterval;
    sh:description
      "The time interval during which the membership is valid."@en,
      "Het tijdsinterval waartijdens het lidmaatschap geldig was."@nl;
    sh:maxCount 1;
    sh:minCount 1;
    sh:name
      "has time interval"@en,
      "heeft interval"@nl;
    sh:nodeKind sh:IRI;#
    sh:path org:memberDuring.

  andb:organizationShape
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:class org:OrganizationalUnit;
    sh:description
      "The union to which the membership applies (apprentices only)."@en,
      "De vakbond waarvoor het lidmaatschap geldt."@nl;
    sh:name
      "has organization"@en,
      "heeft orgnisatie"@nl;
    sh:nodeKind sh:IRI;#
    sh:path org:organization.


  andb:MigrationShape
    a sh:NodeShape;#
    rdfs:isDefinedBy andb:;#
    sh:closed true;#
    sh:ignoredProperties ( rdf:type );#
    sh:name
      "Migration"@en,
      "Vertrek"@nl;
    sh:property
      andb:durationShape,
      andb:locationShape,
      andb:labelShape_en_nl;
    sh:targetClass andb:Migration.

  andb:locationShape
    a sh:PropertyShape;#
    dct:source "andb/departure_place-01 t/m 08";
    rdfs:isDefinedBy andb:;#
    sh:datatype xsd:string;#
    sh:description
      "The location where the person migrated to."@en,
      "De plaats waarnaar de persoon gemigreerd is."@nl;
    sh:maxCount 1;
    sh:minCount 1;
    sh:minLength 1;#
    sh:name
      "has migration destination"@en,
      "heeft migratiebestemming"@nl;
    sh:nodeKind sh:Literal;#
    sh:path schema:location.


  andb:OrganizationShape
    a sh:NodeShape;#
    rdfs:isDefinedBy andb:;#
    sh:closed true;#
    sh:ignoredProperties ( rdf:type );#
    sh:property
      andb:OrganizationShape_name,
      andb:hasUnitShape,
      andb:labelShape_en_nl;
    sh:targetClass org:Organization.

  andb:OrganizationShape_name
    a sh:PropertyShape;#
    dct:source
      "adb/un_name",
      "apprentice/un_name",
      "apprentice/par_un_name";
    rdfs:isDefinedBy andb:;#
    sh:datatype rdf:langString;#
    sh:description
      "The name of the labor union."@en,
      "De naam van de vakbond."@nl;
    sh:languageIn ( "nl" );
    sh:name
      "has name"@en,
      "heeft naam"@nl;
    sh:nodeKind sh:Literal;#
    sh:path schema:name;
    sh:uniqueLanguage true.#

  andb:hasUnitShape
    a sh:PropertyShape;
    rdfs:isDefinedBy andb:;#
    sh:class org:OrganizationalUnit;
    sh:maxCount 1;
    sh:nodeKind sh:IRI;#
    sh:path org:hasUnit.

  andb:unitOfShape
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:or
      ( [ sh:class andb:Union ]
        [ sh:class org:OrganizationalUnit ] );
    sh:maxCount 1;
    sh:nodeKind sh:IRI;#
    sh:path org:unitOf.


  andb:OrganizationalUnitShape
    a sh:NodeShape;#
    rdfs:isDefinedBy andb:;#
    sh:closed true;#
    sh:ignoredProperties ( rdf:type );#
    sh:property
      andb:OrganizationalUnitShape_identifier,
      andb:hasUnitShape,
      andb:labelShape_en_nl,
      andb:unitOfShape;
    sh:targetClass org:OrganizationalUnit.

  andb:OrganizationalUnitShape_identifier
    a sh:PropertyShape;#
    dct:source
      "adb/un_section",
      "adb/un_section_no",
      "andb/un_section-01 t/m 07",
      "andb/un_section_no-01 t/m 07",
      "apprentice/par_un_section",
      "apprentice/par_un_section_no",
      "apprentice/un_section",
      "apprentice/un_section_no";
    rdfs:isDefinedBy andb:;#
    sh:datatype xsd:string;
    sh:description
      "The section number of the labor union section."@en,
      "Het sectie nummer van een vakbondsafdeling van een vakbond."@nl;
    sh:maxCount 1;
    sh:minCount 1;
    sh:minLength 1;#
    sh:name
      "has section number"@en,
      "heeft sectie nummer"@nl;
    sh:nodeKind sh:Literal;#
    sh:path dct:identifier;
    sh:pattern "^[0-9]+$".


