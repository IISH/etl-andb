  andb:PersonShape
    a sh:NodeShape;#
    rdfs:isDefinedBy andb:;#
    sh:closed true;#
    sh:ignoredProperties ( rdf:type );#
    sh:property
      andb:Person_labelShape,
      andb:Person_nameShape,
      andb:additionalNameShape,
      andb:birthDateShape,
      andb:birthPlaceShape,
      andb:cardShape,
      andb:deathDateShape,
      andb:education,
      andb:familyNameShape,
      andb:firstNameShape,
      andb:genderShape,
      andb:hasMembershipShape,
      andb:inhabitsShape,
      andb:maidenNamePrefixShape,
      andb:maidenNameShape,
      andb:migrationShape,
      andb:oorlogslevensShape;
    sh:targetClass schema:Person.

  andb:Person_labelShape
    a sh:PropertyShape;#
    rdfs:isDefinedBy andb:;#
    sh:datatype xsd:string;
    sh:maxCount 1;
    sh:minCount 1;
    sh:minLength 1;#
    sh:name
      "has human-readable label"@en,
      "heeft menselijke leesbaar label"@n;;
    sh:nodeKind sh:Literal;#
    sh:path rdfs:label.

