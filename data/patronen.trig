  andb:cardShape
    a sh:PropertyShape;#
    dct:source
      "unique/adb_identifier",
      "unique/sdn_adm_id1",
      "unique/sdn_adm_id2",
      "unique/sdn_adm_id3",
      "unique/mbr_adm_id1",
      "unique/mbr_adm_id2",
      "unique/mbr_adm_id3";
    rdfs:isDefinedBy andb:;#
    sh:or
      ( [ sh:class andb:MembershipCard ]
        [ sh:class andb:StudentCard ] );
    sh:description "A person can have one or more cards."@en;
    sh:minCount 1;
    sh:name
      "has card"@en,
      "heeft kaart"@nl;
    sh:nodeKind sh:IRI;#
    sh:path foaf:isPrimaryTopicOf.

  andb:deathDateShape
    a sh:PropertyShape;#
    dct:source
      "apprentice/par_death_day/month/year",
