prefix dct: <http://purl.org/dc/terms/>
prefix foaf: <http://xmlns.com/foaf/0.1/>
prefix geo: <http://www.opengis.net/ont/geosparql#>
prefix gml: <http://www.opengis.net/ont/gml#>
prefix nen3610: <http://definities.geostandaarden.nl/def/nen3610#>
prefix owl: <http://www.w3.org/2002/07/owl#>
prefix prov: <http://www.w3.org/ns/prov#>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix sdo: <https://schema.org/>
prefix sh: <http://www.w3.org/ns/shacl#>
prefix skos: <http://www.w3.org/2004/02/skos/core#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>

prefix bag: <https://bag2.basisregistraties.overheid.nl/bag/def>
prefix graph: <https://bag2.basisregistraties.overheid.nl/bag/id/graph/>
prefix shape: <https://bag2.basisregistraties.overheid.nl/bag/id/shape/>
prefix status: <https://bag2.basisregistraties.overheid.nl/bag/id/status/>

graph:shapes {
  shape:AdresseerbaarObject
    sh:property
      sh:hoofdadres,
      sh:nevenadres;
    sh:targetClass bag:AdresseerbaarObject.

  sh:hoofdadres
    sh:class bag:Nummeraanduiding;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path bag:hoofdadres.

  sh:nevenadres
    sh:class bag:Nummeraanduiding;
    sh:path bag:nevenadres.


  shape:Geometry
    sh:property shape:wktLiteral;
    sh:targetClass geo:Geometry.

  shape:wktLiteral
    sh:datatype geo:wktLiteral;
    sh:description "Iedere geometrie heeft precies 1 serialisatie."@nl;
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "heeft WKT serialisatie"@nl;
    sh:path geo:asWKT.


  shape:Identificatie
    sh:property
      shape:lokaalId,
      shape:namespace;
    sh:targetClass nen3610:Identificatie.

  shape:lokaalId
    sh:datatype xsd:string;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path nen3610:lokaalID.

  shape:namespace
    sh:datatype xsd:string;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path nen3610:namespace;
    sh:pattern "^NL.IMBAG.".


  shape:Ligplaats
    sh:property
      shape:Ligplaats_identificatie,
      shape:Ligplaats_isPrimaryTopicOf;
    sh:targetClass bag:Ligplaats.

  shape:Ligplaats_isPrimaryTopicOf
    sh:class bag:LigplaatsRegistratie;
    sh:minCount 1;
    sh:path foaf:isPrimaryTopicOf.

  shape:Ligplaats_identificatie
    dct:source "Grondslagen Basis Gebouwen Registratie versie 2.3"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#761-identificatie"^^xsd:anyURI;
    sh:class nen3610:Identificatie;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path nen3610:identificatie;
    skos:definition "De unieke aanduiding van een ligplaats."@nl;
    skos:scopeNote "Elke Ligplaats waarvan gegevens zijn opgenomen in de basisregistratie adressen en gebouwen wordt uniek aangeduid door middel van een identificatiecode."@nl.

  shape:LigplaatsRegistratie
    sh:property
      shape:LigplaatsRegistratie_documentdatum,
      shape:LigplaatsRegistratie_documentnummer,
      shape:LigplaatsRegistratie_geconstrateerd,
      shape:LigplaatsRegistratie_geometrie,
      shape:LigplaatsRegistratie_status;
    sh:tagetClass bag:LigplaatsRegistratie.

  shape:LigplaatsRegistratie_documentdatum
    dct:source "Grondslagen Basis Gebouwen Registratie versie 2.3"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#765-documentdatum"^^xsd:anyURI;
    sh:datatype xsd:date;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path dct:created;
    skos:definition "De datum waarop het brondocument is vastgesteld op basis waarvan een opname, mutatie of een verwijdering van gegevens ten aanzien van een ligplaats heeft plaatsgevonden."@nl;
    skos:scopeNote "In het geval het brondocument een besluit van een bestuursorgaan betreft, dan is de datum van het brondocument de datum waarop het besluit is genomen."@nl.

  shape:LigplaatsRegistratie_documentnummer
    dct:source "Grondslagen Basis Gebouwen Registratie versie 2.3"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#766-documentnummer"^^xsd:anyURI;
    sh:datatype xsd:string;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path dct:identifier;
    sh:pattern "[0-9]{1,40}";
    skos:definition "De unieke aanduiding van het brondocument op basis waarvan een opname, mutatie of een verwijdering van gegevens ten aanzien van een ligplaats heeft plaatsgevonden binnen een gemeente."@nl.

  shape:LigplaatsRegistratie_geconstrateerd
    dct:source "Grondslagen Basis Gebouwen Registratie versie 4.0"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#764-geconstateerd"^^xsd:anyURI;
    sh:datatype xsd:boolean;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path bag:geconstateerd;
    skos:definition "Een aanduiding waarmee kan worden aangegeven dat een ligplaats in de registratie is opgenomen als gevolg van een feitelijke constatering, zonder dat er op het moment van opname sprake was van een regulier brondocument voor deze opname."@nl;
    skos:scopeNote "Het feitelijk bestaan van een ligplaats dient daadwerkelijk te worden geconstateerd. Het gaat dan bijvoorbeeld om het feitelijk permanent gebruik van een plaats in het water dat binnen andere processen aan het licht is gekomen."@nl.

  shape:LigplaatsRegistratie_geometrie
    dct:source "Grondslagen Basis Gebouwen Registratie versie 2.3"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#763-geometrie"^^xsd:anyURI;
    sh:class gml:Surface;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path bag:geometrie;
    skos:definition "De tweedimensionale geometrische representatie van de omtrekken van een ligplaats."@nl.

  shape:LigplaatsRegistratie_status
    dct:source "Grondslagen Basis Gebouwen Registratie versie 4.0"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#762-status"^^xsd:anyURI;
    sh:class bag:StatusPlaats;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path bag:status;
    skos:definition "De fase van de levenscyclus van een ligplaats waarin de betreffende ligplaats zich bevindt."@nl;
    skos:scopeNote "De BAG bevat gegevens over zowel daadwerkelijk aanwezige ligplaatsen als ingetrokken ligplaatsen. Om het onderscheid tussen de stadia in de levenscyclus van de ligplaats te kunnen maken, wordt gebruik gemaakt van dit statusgegeven."@nl.


  shape:Registratie
    sh:property
      shape:formeleHistorieBegin,
      shape:formeleHistorieEind,
      shape:formeleLevensduur,
      shape:materiëleHistorieBegin,
      shape:materiëleHistorieEind,
      shape:materiëleLevensduur,
      shape:primaryTopic;
    sh:targetClass foaf:Document.

  shape:formeleHistorieBegin
    sh:datatype xsd:dateTime;
    sh:description "Wordt gebruikt om de formele historie mee aan de duiden."@nl;
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "heeft begin registratie"@nl;
    sh:path prov:generatedAtTime.

  shape:formeleHistorieEind
    sh:datatype xsd:dateTime;
    sh:description "Wordt gebruikt om de formele historie mee aan de duiden."@nl;
    sh:maxCount 1;
    sh:name "heeft eind registratie"@nl;
    sh:path prov:invalidatedAtTime.

  shape:formeleLevensduur
    sh:class foaf:Document;
    sh:description "Wordt gebruikt om de formele levensduur mee aan de duiden."@nl;
    sh:maxCount 1;
    sh:name "heeft vorige geregistreerde versie"@nl;
    sh:path nen3610:vorigeGeregistreerdeVersie.

  shape:materiëleHistorieBegin
    sh:datatype xsd:dateTime;
    sh:description "Wordt gebruikt om de materiële historie mee aan de duiden."@nl;
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "heeft begin geldigheid"@nl;
    sh:path nen3610:beginGeldigheid.

  shape:materiëleHistorieEind
    sh:datatype xsd:dateTime;
    sh:description "Wordt gebruikt om de materiële historie mee aan de duiden."@nl;
    sh:maxCount 1;
    sh:name "heeft eind geldigheid"@nl;
    sh:path nen3610:eindGeldigheid.

  shape:materiëleLevensduur
    sh:class foaf:Document;
    sh:description "Wordt gebruikt om de materiële levensduur mee aan de duiden."@nl;
    sh:maxCount 1;
    sh:name "heeft vorige geldige versie"@nl;
    sh:path nen3610:vorigeGeldigeVersie.

  shape:primaryTopic
    sh:maxCount 1;
    sh:minCount 1;
    sh:nodeType sh:IRI;
    sh:path foaf:primaryTopic.


  shape:Standplaats
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#77-standplaats"^^xsd:anyURI;
    sh:property
      shape:Standplaats_identificatie,
      shape:Standplaats_isPrimaryTopicOf;
    sh:targetClass bag:Standplaats.

  shape:Standplaats_identificatie
    dct:source "Grondslagen Basis Gebouwen Registratie versie 2.3"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#771-identificatie"^^xsd:anyURI;
    sh:class nen3610:Identificatie;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path nen3610:identificatie;
    skos:definition "De unieke aanduiding van een standplaats."@nl;
    skos:scopeNote "Elke standplaats waarvan gegevens zijn opgenomen in de BAG wordt uniek aangeduid door middel van een identificatiecode."@nl.

  shape:Standplaats_isPrimaryTopicOf
    sh:class bag:StandplaatsRegistratie;
    sh:minCount 1;
    sh:path foaf:isPrimaryTopicOf.

  shape:StandplaatsRegistratie
    sh:property
      shape:StandplaatsRegistratie_documentdatum,
      shape:StandplaatsRegistratie_documentnummer,
      shape:StandplaatsRegistratie_geconstrateerd,
      shape:StandplaatsRegistratie_geometrie,
      shape:StandplaatsRegistratie_status;
    sh:tagetClass bag:StandplaatsRegistratie.

  shape:StandplaatsRegistratie_documentdatum
    dct:source "Grondslagen Basis Gebouwen Registratie versie 2.3"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#775-documentdatum"^^xsd:anyURI;
    sh:datatype xsd:date;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path dct:created;
    skos:definition "De datum waarop het brondocument is vastgesteld op basis waarvan een opname, mutatie of een verwijdering van gegevens ten aanzien van een standplaats heeft plaatsgevonden."@nl;
    skos:scopeNote "In het geval het brondocument een besluit van een bestuursorgaan betreft, dan is de datum van het brondocument de datum waarop het besluit is genomen."@nl.

  shape:StandplaatsRegistratie_documentnummer
    dct:source "Grondslagen Basis Gebouwen Registratie versie 2.3"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#776-documentnummer"^^xsd:anyURI;
    sh:datatype xsd:string;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path dct:identifier;
    sh:pattern "[0-9]{1,40}";
    skos:definition "De unieke aanduiding van het brondocument op basis waarvan een opname, mutatie of een verwijdering van gegevens ten aanzien van een standplaats heeft plaatsgevonden binnen een gemeente."@nl.

  shape:StandplaatsRegistratie_geconstrateerd
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#774-geconstateerd"^^xsd:anyURI;
    sh:datatype xsd:boolean;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path bag:geconstateerd;
    skos:definition "Een aanduiding waarmee kan worden aangegeven dat een standplaats in de registratie is opgenomen als gevolg van een feitelijke constatering, zonder dat er op het moment van opname sprake was van een regulier brondocument voor deze opname."@nl;
    skos:scopeNote "Het feitelijk bestaan van een standplaats dient daadwerkelijk te worden geconstateerd. Het gaat dan bijvoorbeeld om het feitelijk gebruik van (een gedeelte van) een terrein dat binnen andere processen aan het licht is gekomen."@nl.

  shape:StandplaatsRegistratie_geometrie
    dct:source "Grondslagen Basis Gebouwen Registratie versie 2.3"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#773-geometrie"^^xsd:anyURI;
    sh:class gml:Surface;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path bag:geometrie;
    skos:definition "De tweedimensionale geometrische representatie van de omtrekken van een standplaats."@nl.

  shape:StandplaatsRegistratie_status
    dct:source "Grondslagen Basis Gebouwen Registratie versie 4.0"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#772-status"^^xsd:anyURI;
    sh:class bag:StatusPlaats;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path bag:status;
    skos:definition "De fase van de levenscyclus van een standplaats waarin de betreffende standplaats zich bevindt."@nl;
    skos:scopeNote "De BAG bevat gegevens over zowel daadwerkelijk aanwezige standplaatsen als ingetrokken standplaatsen. Om het onderscheid tussen de stadia in de levenscyclus van de standplaats te kunnen maken, wordt gebruik gemaakt van dit statusgegeven."@nl.


  shape:Nummeraanduiding
    sh:property
      shape:Nummeraanduiding_identificatie,
      shape:Nummeraanduiding_isPrimaryTopicOf;
    sh:targetClass bag:Nummeraanduiding.

  shape:Nummeraanduiding_identificatie
    dct:source "Grondslagen Basis Registratie Adressen versie 2.0"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#731-identificatie"^^xsd:anyURI;
    sh:class nen3610:Identificatie;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path nen3610:identificatie;
    skos:definition "De unieke aanduiding van een nummeraanduiding."@nl;
    skos:scopeNote "Elke nummeraanduiding waarvan gegevens zijn opgenomen in de basisregistratie adressen en gebouwen, wordt uniek aangeduid door middel van een identificatiecode."@nl.

  shape:Nummeraanduiding_isPrimaryTopicOf
    sh:class bag:NummeraanduidingRegistratie;
    sh:minCount 1;
    sh:path foaf:isPrimaryTopicOf.

  shape:NummeraanduidingRegistratie
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#73-nummeraanduiding"^^xsd:anyURI;
    sh:property
      shape:NummeraanduidingRegistratie_documentdatum,
      shape:NummeraanduidingRegistratie_documentnummer,
      shape:NummeraanduidingRegistratie_geconstateerd,
      shape:NummeraanduidingRegistratie_status,
      shape:huisletter,
      shape:huisnummer,
      shape:huisnummertoevoeging,
      shape:openbareRuimte,
      shape:postcode;
    sh:targetClass bag:NummeraanduidingRegistratie.

  shape:NummeraanduidingRegistratie_documentdatum
    dct:source "Grondslagen Basis Gebouwen Registratie versie 2.3"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#739-documentdatum"^^xsd:anyURI;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path dct:created;
    skos:definition "De datum waarop het brondocument is vastgesteld op basis waarvan een opname, mutatie of een verwijdering van gegevens ten aanzien van een nummeraanduiding heeft plaatsgevonden."@nl;
    skos:scopeNote "In het geval het brondocument een besluit van een bestuursorgaan betreft, dan is de datum van het brondocument de datum waarop het besluit is genomen."@nl.

  shape:NummeraanduidingRegistratie_documentnummer
    dct:source "Grondslagen Basis Gebouwen Registratie versie 2.3"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#7310-documentnummer"^^xsd:anyURI;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path dct:identifier;
    sh:pattern "[0-9]{1,40}";
    skos:definition "De unieke aanduiding van het brondocument op basis waarvan een opname, mutatie of een verwijdering van gegevens ten aanzien van een nummeraanduiding heeft plaatsgevonden binnen een gemeente."@nl.

  shape:NummeraanduidingRegistratie_geconstateerd
    dct:source "Grondslagen Basis Registratie Adressen versie 4.0"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#738-geconstateerd"^^xsd:anyURI;
    sh:datatype xsd:boolean;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path bag:geconstateerd;
    skos:definition "Een aanduiding waarmee kan worden aangegeven dat een nummeraanduiding in de registratie is opgenomen als gevolg van een feitelijke constatering, zonder dat er op het moment van opname sprake was van een regulier brondocument voor deze opname."@nl;
    skos:scopeNote "Het feitelijk bestaan van een nummeraanduiding dient daadwerkelijk te worden geconstateerd. Het gaat dan bijvoorbeeld om door burgers aangebrachte nummeraanduidingen die binnen andere processen aan het licht zijn gekomen."@nl.

  shape:NummeraanduidingRegistratie_status
    dct:source "Grondslagen Basis Registratie Adressen versie 4.0"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#737-status"^^xsd:anyURI;
    sh:class bag:StatusNaamgeving;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path bag:status;
    skos:definition "De fase van de levenscyclus van een nummeraanduiding waarin de betreffende nummeraanduiding zich bevindt."@nl;
    skos:scopeNote "De basisregistratie adressen en gebouwen bevat gegevens over zowel bestaande nummeraanduidingen als ingetrokken nummeraanduidingen. Om het onderscheid tussen de stadia in de levenscyclus van de nummeraanduiding te kunnen maken, wordt gebruik gemaakt van dit statusgegeven."@nl.

  shape:huisletter
    dct:source "Grondslagen Basis Registratie Adressen versie 2.0"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#733-huisletter"^^xsd:anyURI;
    sh:datatype xsd:string;
    sh:maxCount 1;
    sh:path bag:huisletter;
    sh:pattern "[A-Z][a-z]{1}";
    skos:definition "Een door of namens het gemeentebestuur ten aanzien van een adresseerbaar object toegekende toevoeging aan een huisnummer in de vorm van een alfanumeriek teken."@nl.

  shape:huisnummer
    sh:datatype xsd:positiveInteger;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path bag:huisnummer;
    sh:pattern "[0-9]{1,5}".

  shape:huisnummertoevoeging
    sh:datatype xsd:string;
    sh:description "Maximaal vier alfanumerieke tekens bestaande uit een combinatie van hoofdletters (A – Z), kleine letters (a – z) en/of cijfers (0 – 9)."@nl;
    sh:maxCount 1;
    sh:path bag:huisnummertoevoeging;
    sh:pattern "[0-9A-Za-z]{1,4}".

  shape:openbareRuimte
    sh:class bag:OpenbareRuimte;
    sh:maxCount 1;
    sh:minCount 1;
    sh:path bag:openbareRuimte.

  shape:postcode
    sh:datatype xsd:string;
    sh:description "Combinatie van vier cijfers (0 – 9) en twee hoofdletters (A – Z)."@nl;
    sh:maxCount 1;
    sh:path bag:postcode;
    sh:pattern "[0-9]{4}[A-Z]{2}".


  shape:OpenbareRuimte
